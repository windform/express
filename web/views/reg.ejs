<%- include header %>
<div class="container">
<legend><%- title%></legend>
<form class="form-horizontal" method="post">
  <div class="form-group">
    <label for="username" class="col-sm-2 control-label">用户名</label>
    <div class="col-sm-5">
      <input type="text" class="form-control" id="username" placeholder="Email">
    </div>
  </div>
  <div class="form-group">
    <label for="password" class="col-sm-2 control-label">口令</label>
    <div class="col-sm-5">
      <input type="password" class="form-control" id="password" placeholder="Password">
    </div>
  </div>
  <div class="form-group">
    <label for="password-repeat" class="col-sm-2 control-label">重复输入口令</label>
    <div class="col-sm-5">
      <input type="password" class="form-control" id="password-repeat" placeholder="Password">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">注册</button>
    </div>
  </div>
</form>
</div>
<%- include footer %>